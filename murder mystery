Murder Mystery

Step one:
SELECT *
FROM crime_scene_report
 where type = 'murder'
 and city = 'SQL City'
 and date = '20180115'

Step 2:
SELECT *
FROM person
where address_street_name = 'Northwestern Dr'
order by address_number DESC
limit 50

Step 3:
SELECT *
FROM person
where address_street_name = 'Franklin Ave'
and name like '%Annabel%'
limit 50

Step 4:
SELECT *
FROM person
where id IN (14887,16371)
limit 50

Step 5:
SELECT *
FROM interview
where person_id IN (14887,16371)
limit 50

Step 6:
SELECT *
FROM drivers_license
where plate_number like '%H42W%'
limit 50

Step 7:
SELECT *
FROM drivers_license as DL
inner join person as PR on DL.id = PR.license_id
where plate_number like '%H42W%'
limit 50

Step 8:
SELECT Pr.*
FROM drivers_license as DL
inner join person as PR on DL.id = PR.license_id
inner join get_fit_now_member as GFM on PR.id = GFM.person_id
where plate_number like '%H42W%'
limit 50

Step 9:
SELECT Pr.*, GFM.*
FROM drivers_license as DL
inner join person as PR on DL.id = PR.license_id
inner join get_fit_now_member as GFM on PR.id = GFM.person_id
where plate_number like '%H42W%'
limit 50

Ckeck yor solution
INSERT INTO solution VALUES (1, 'Jeremy Bowers');
        
        SELECT value FROM solution;


Step 10:

SELECT *
from interview
where person_id = 67318

Step 11:
SELECT PR.*
from drivers_license as DL
inner join person as PR on DL.id = PR.license_id
where height >=65
and height <=67
and car_make = 'Tesla'
and car_model = 'Model S'
and gender = 'female'

Step 12:
SELECT PR.*, FBC.*
from drivers_license as DL
inner join person as PR on DL.id = PR.license_id
inner join facebook_event_checkin as FBC on FBC.person_id = PR.id
where height >=65
and height <=67
and car_make = 'Tesla'
and car_model = 'Model S'
and gender = 'female'


INSERT INTO solution VALUES (1, 'Miranda Priestly');

